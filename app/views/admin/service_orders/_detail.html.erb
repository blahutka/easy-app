<div class="modal-body">
  <div class="row-fluid">
    <div class="span6 well">
      <h6>Zákazník</h6>
      <address>
        <strong><%= @service_order.customer_company_name %></strong><br>
        <%= @service_order.customer_street %>, <%= @service_order.customer_city %>, <%= @service_order.customer_zip %>
      </address>
    </div>

    <div class="span6"><br>
      <h6>Poskytovatel služby</h6>
      <address>
        <strong><%= @service_order.provider_company_name %></strong><br>
        <%= @service_order.provider_street %>, <%= @service_order.provider_city %>, <%= @service_order.provider_zip %>
      </address>
    </div>
  </div>

  <div class="row-fluid">
    <hr>
    <div class="span11">

      <strong>Kategorie</strong> <%= @service_order.request_product_category %> <br>
      <strong>Značka</strong> <%= @service_order.request_product_brand %>
      <br>
      <strong>Popis závady</strong><%= @service_order.request_defect_description %>
      <br>

    </div>
  </div>
</div>
<div class="form-actions">
  <%= link_to 'Zavřít', '#', :class => 'btn', 'data-dismiss' => "modal" %>
</div>