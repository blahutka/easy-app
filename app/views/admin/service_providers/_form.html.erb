<%= simple_form_for [:admin, @service_provider], :html => {class: 'form-horizontal'} do |f| %>
    <div class="modal-body">
      <%= f.input :company_name %>
      <%= f.input :phone %>
      <%= f.input :dic %>
      <%= f.input :ico %>
      <legend>Adresa</legend>
      <%= f.input :street %>
      <%= f.input :city %>
      <%= f.input :zip %>
      <%= f.input :country %>
    </div>
    <div class="form-actions">
      <%= f.button :submit %>
    </div>
<% end %>